# Base URL for the application
DOTTY_BASE_URL=http://localhost:8080

# WebSocket URL for Dotty
DOTTY_WS_URL=ws://localhost:8080/dotty

# Auth0 URIs for authentication and signout
AUTH0_DISCOVERY_URI=http://localhost:8080/oauth2/authorization/auth0
AUTH0_SIGNOUT_URI=http://localhost:8080/api/dotty/oauth2/auth0/logout

# Auth0 credentials (masking sensitive values)
AUTH0_CLIENT_ID=your-auth0-client-id-here
AUTH0_CLIENT_SECRET=your-auth0-client-secret-here
AUTH0_REFRESH_URI=http://localhost:8080/api/dotty/oauth2/auth0/refresh

# Schwab API endpoint and credentials (masking sensitive values)
CHSH_AUTHORIZATION_ENDPOINT=your-schwab-authorization-endpoint-here
CHSH_AUTHORIZATION_KEY=your-schwab-authorization-key-here
CHSH_AUTHORIZATION_REDIRECT_URI=your-redirect-url-here

# Alpaca API endpoint (no sensitive info here)
ALPACA_AUTHORIZATION_ENDPOINT=/api/dotty/userTradingAccount/alpaca